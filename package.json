{
    "name": "fozbot-mta",
    "version": "1.0.0",
    "description": "Bot de tickets para MTA em Discord",
    "main": "src/index.js",
    "type": "module",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "deploy": "node src/deploy-commands.js",
        "web": "node src/web/server.js",
        "web:dev": "nodemon src/web/server.js",
        "panel:install": "cd web && npm install",
        "panel:dev": "cd web && npm run dev",
        "panel:build": "cd web && npm run build",
        "generate-password": "node generate-password.js"
    },
    "keywords": [
        "discord",
        "bot",
        "mta",
        "tickets"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "discord-html-transcripts": "^3.2.0",
        "discord.js": "^14.16.3",
        "better-sqlite3": "^11.6.0",
        "dotenv": "^16.4.7",
        "yaml": "^2.6.1",
        "express": "^4.18.2",
        "socket.io": "^4.6.1",
        "cors": "^2.8.5",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6"
    },
    "devDependencies": {
        "nodemon": "^3.1.7"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
